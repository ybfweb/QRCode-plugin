{
  "name": "QRCode-plugin",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "将chrome地址栏转换成二维码",
  "browser_action": {
    "default_icon": "static/images/icon.png",
    "default_title": "QRCode-plugin",
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "static/images/icon.png",
    "16": "static/images/icon.png",
    "48": "static/images/icon.png"
  },
  "permissions": [ "tabs"]
}